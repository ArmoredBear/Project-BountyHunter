; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Bounty Hunter"
run/main_scene="res://Common/Main_Menu.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"
boot_splash/minimum_display_time=2000

[autoload]

GameManager="*res://Common/General Scritps/GameManager.cs"
PlayerDataAutoload="*res://Player/PlayerScripts/Player_Data_Autoload.cs"
Player="*res://Player/PlayerPrefabs/Player.tscn"
Messenger="*res://Common/General Scritps/Messenger.cs"
Inventory="*res://Common/Inventory/Inventory.cs"
Panku="*res://addons/panku_console/console.tscn"
SaveSystem="*res://Common/SaveSystem.tscn"
DialogueManager="*res://Common/DialogBox/ScriptsUI/DialogueManager.cs"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/extend_to_title=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true

[dotnet]

project/assembly_name="Project Bounty Hunter"

[editor_plugins]

enabled=PackedStringArray("res://addons/panku_console/plugin.cfg")

